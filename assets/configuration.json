{
    "types": {
        "animal": "com.innerfunction.semo.test.Animal",
        "color": "com.innerfunction.semo.test.Color",
        "fruit": "com.innerfunction.semo.test.Fruit",
        "plant": "com.innerfunction.semo.test.Plant",
        "forest": "com.innerfunction.semo.test.Forest"
    },
    "names": {
        "monkey": {
            "semo:type": "animal",
            "likes": "@named:banana"
        },
        "banana": {
            "semo:type": "fruit",
            "color": "@named:yellow"
        },
        "yellow": {
            "semo:type": "color",
            "value": "#00FFFF"
        },
        "tree": {
            "semo:type": "plant",
            "contains": [
                "@named:monkey",
                "@named:banana",
                {
                    "semo:type": "animal",
                    "name": "Parrot"
                }
            ]
        },
        "jungle": {
            "semo:type": "forest",
            "thingsInTheForest": {
                "tree": "@named:tree",
                "jaguar": {
                    "semo:type": "animal",
                    "name": "Jaguar"
                }
            }
        }
    }
}
